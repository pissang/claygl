{
    "meta" : {
        "name" : "Curl Noise"
    },
    "type" : "compositor",
    "shaders" : {
        "curlnoise" : "#url(curlnoise.essl)"
    },
    "parameters" : {
        "common" : {
            "width" : 256,
            "height" : 256,
            "type" : "FLOAT",
            "minFilter" : "NEAREST",
            "magFilter" : "NEAREST",
            "useMipmap" : false
        }
    },
    "textures" : {},
    "nodes" : [
        {
            "name" : "spawn",
            "shader" : "#source(curlnoise.spawn)",
            "type" : "filter",
            "outputs" : {
                "position" : {
                    "parameters" : "#common"
                }
            }
        },
        {
            "name" : "noise",
            "shader" : "#source(curlnoise.noise)",
            "type" : "filter",
            "outputs" : {
                "noise" : {
                    "parameters" : "#common"
                }
            }
        },
        {
            "name" : "particle",
            "shader" : "#source(curlnoise.particle)",
            "type" : "filter",
            "output" : true,
            "parameters" : {
                "noiseTextureSize" : 256
            },
            "inputs" : {
                "particleTexture" : {
                    "node" : "particle",
                    "pin" : "position"
                },
                "spawnTexture" : {
                    "node" : "spawn",
                    "pin" : "position"
                },
                "noiseTexture" : {
                    "node" : "noise",
                    "pin" : "noise"
                }
            },
            "outputs" : {
                "position" : {
                    "parameters" : "#common",
                    "keepLastFrame" : true
                }
            }
        }
    ]
}